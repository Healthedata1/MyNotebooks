peats": false, "required": true, "text": "Have you engaged in illegal activities in order to obtain drugs?", "type": "choice"}, {"extension": [{"url": "http://fhir.org/guides/argonaut-questionnaire/StructureDefinition/extension-itemOrder", "valueUnsignedInt": 9}], "code": [{"code": "68533-9", "system": "http://loinc.org"}], "linkId": "g1.q9", "option": [{"extension": [{"url": "http://fhir.org/guides/argonaut-questionnaire/StructureDefinition/extension-score", "valueDecimal": 0}, {"url": "http://fhir.org/guides/argonaut-questionnaire/StructureDefinition/extension-itemOrder", "valueUnsignedInt": 0}], "valueCoding": {"code": "LA 32-8 ", "display": "No", "system": "http://loinc.org"}}, {"extension": [{"url": "http://fhir.org/guides/argonaut-questionnaire/StructureDefinition/extension-score", "valueDecimal": 1}, {"url": "http://fhir.org/guides/argonaut-questionnaire/StructureDefinition/extension-itemOrder", "valueUnsignedInt": 1}], "valueCoding": {"code": "LA33-6 ", "display": "Yes", "system": "http://loinc.org"}}], "prefix": "9.", "repeats": false, "required": true, "text": "Have you ever experienced withdrawal symptoms (felt sick) when you stopped taking drugs?", "type": "choice"}, {"extension": [{"url": "http://fhir.org/guides/argonaut-questionnaire/StructureDefinition/extension-itemOrder", "valueUnsignedInt": 10}], "code": [{"code": "68534-7", "system": "http://loinc.org"}], "linkId": "g1.q10", "option": [{"extension": [{"url": "http://fhir.org/guides/argonaut-questionnaire/StructureDefinition/extension-score", "valueDecimal": 0}, {"url": "http://fhir.org/guides/argonaut-questionnaire/StructureDefinition/extension-itemOrder", "valueUnsignedInt": 0}], "valueCoding": {"code": "LA 32-8 ", "display": "No", "system": "http://loinc.org"}}, {"extension": [{"url": "http://fhir.org/guides/argonaut-questionnaire/StructureDefinition/extension-score", "valueDecimal": 1}, {"url": "http://fhir.org/guides/argonaut-questionnaire/StructureDefinition/extension-itemOrder", "valueUnsign