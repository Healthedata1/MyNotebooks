    "import itertools \n",
    "\n",
    "'''\n",
    "df1 = pd.DataFrame({'key': ['foo', 'bar','fizz'],\n",
    "                    'value': [1, 2,11]})\n",
    "df2 = pd.DataFrame({'key': ['foo', 'bar', 'baz'],\n",
    "                    'value': [5, 6, 7]})\n",
    "\n",
    "df1 = pd.read_excel(r'test_files/Book1.xlsx')\n",
